{
  "name": "@sxltd/dendron-plugin-views",
  "version": "0.3.0",
  "private": true,
  "workspaces": {
    "nohoist": [
      "webpack",
      "webpack/**"
    ]
  },
  "dependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@sxltd/common-all": "workspace:*",
    "@sxltd/common-frontend": "workspace:*",
    "@sxltd/common-server": "workspace:*",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@types/cytoscape": "^3.23.0",
    "@types/cytoscape-popper": "^2.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "antd": "^4.21.4",
    "babel-jest": "^26.6.0",
    "babel-plugin-named-asset-import": "^0.3.7",
    "babel-preset-react-app": "^10.0.0",
    "cytoscape": "^3.23.0",
    "cytoscape-euler": "^1.2.2",
    "cytoscape-popper": "^2.0.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^5.6.5",
    "jest": "26.6.0",
    "jest-circus": "26.6.0",
    "jest-resolve": "26.6.0",
    "jest-watch-typeahead": "0.6.1",
    "lodash": "^4.17.21",
    "mermaid": "^9.1.2",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dev-utils": "^11.0.3",
    "react-dom": "^17.0.2",
    "react-refresh": "^0.8.3",
    "resolve": "1.18.1",
    "tippy.js": "^6.3.7",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "serve": "NODE_ENV=development webpack serve",
    "start": "yarn build:index && node scripts/start.js",
    "setup": "yarn build:index && yarn build:styles",
    "build": "yarn setup && NODE_ENV=development webpack && yarn build:styles && yarn copy",
    "copy": "node scripts/copy.js",
    "build:styles": "node scripts/buildStyles.js",
    "build:index": "node scripts/buildIndex.js",
    "test": "echo 0",
    "test-local": "node scripts/test.js",
    "clean-old": "yarn rimraf build public/static public/*.css public/index.html node_modules || 0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "/workspaces/dendron/node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/lodash": "^4.14.176",
    "@types/mermaid": "^8.2.9",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "mini-css-extract-plugin": "^2.9.4",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "sass": "^1.94.2",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
